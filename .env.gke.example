# GKE Environment Variables (Kubernetes internal DNS)
# Copy this to .env.gke for GKE deployment

# Database Configuration
DATABASE_URL=mysql+pymysql://user:password@mysql-service:3306/dbname

# Service URLs (Kubernetes internal service names)
POOL_SERVICE_URL=http://pool-service:8000
MATCH_SERVICE_URL=http://match-service:8000
DECISION_SERVICE_URL=http://decision-service:8000
USER_SERVICE_URL=http://user-service:8000

# API Configuration
API_V1_PREFIX=/api/v1
PROJECT_NAME=Nice2MeetU-Match

# Security
SECRET_KEY=your-secret-key
CORS_ORIGINS=["*"]
